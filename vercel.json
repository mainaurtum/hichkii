{
  "buildCommand": "npm install --include=dev && npx vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install --include=dev",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
